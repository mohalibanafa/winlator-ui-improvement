<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <gradient android:startColor="#3300D1FF" android:endColor="#5500D1FF" android:angle="90" />
            <stroke android:width="1dp" android:color="@color/colorStroke" />
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="9dp" />
            <solid android:color="#0F9EC7" />
        </shape>
    </item>
</layer-list>